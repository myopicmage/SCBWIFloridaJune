@model IEnumerable<SCBWIFloridaJune.Models.Track>

@{
    ViewBag.Title = "Index";
}

<h2>Track Management</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Presenters)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Display)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Presenters)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Display)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TrackID }) |
            @Html.ActionLink("Details", "Details", new { id=item.TrackID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TrackID })
        </td>
    </tr>
}

</table>

<p>
    @Html.ActionLink("Back to list", "Index", "Manage")
</p>